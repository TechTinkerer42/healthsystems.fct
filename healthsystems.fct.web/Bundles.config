<?xml version="1.0"?>
<bundleConfig ignoreIfDebug="true" ignoreIfLocal="true">
  <cssBundles>
    <add bundlePath="~/css/shared">
        <files>
			<add filePath="~/bower_components/bootstrap/dist/css/bootstrap.css"></add>
			<add filePath="~/bower_components/font-awesome/css/font-awesome.css"></add>
			<add filePath="~/bower_components/bootstrap-datepicker/dist/css/bootstrap-datepicker.standalone.css"></add>
			<add filePath="~/bower_components/toastr/toastr.css"></add>
			<add filePath="~/Content/css/dashboard.css"></add>
			<add filePath="~/Content/css/site.css"></add>
			<add filePath="~/Content/css/table-style.css"></add>
