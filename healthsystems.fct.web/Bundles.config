	    <add filePath="~/bower_components/angular-smart-table/dist/smart-table.js"></add>
	    <add filePath="~/bower_components/bootstrap-datepicker/dist/js/bootstrap-datepicker.js"></add>

		<add filePath="~/bower_components/bootstrap-validator/dist/validator.js"></add>
		<add filePath="~/bower_components/moment/moment.js"></add>
		<add filePath="~/bower_components/jquery.inputmask/dist/jquery.inputmask.bundle.js"></add>
		<add filePath="~/bower_components/ngmodel-format/src/ngmodel.format.js"></add>
		<add filePath="~/bower_components/toastr/toastr.js"></add>

	    <add filePath="~/Content/js/app.js"></add>
	    <add filePath="~/Content/js/routes.js"></add>

