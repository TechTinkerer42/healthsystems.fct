	    <add filePath="~/Content/js/controllers/LoginController.js"></add>
	    <add filePath="~/Content/js/controllers/DashboardController.js"></add>
	    <add filePath="~/Content/js/controllers/MasterfileController.js"></add>
	    <add filePath="~/Content/js/controllers/masterfiles/user/UserController.js"></add>
	    <add filePath="~/Content/js/controllers/masterfiles/user/UserManageController.js"></add>
      </files>
    </add>
